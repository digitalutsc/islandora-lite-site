    "require-dev": {
      "drupal/devel": "^5.0"
    },
    "conflict": {
        "drupal/drupal": "*",
        "drupal/core": "<=8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
          "sort-packages": true,
          "preferred-install": "dist",
          "allow-plugins": {
              "composer/installers": true,
              "drupal/core-composer-scaffold": true,
              "drupal/core-project-message": true,
              "cweagans/composer-patches": true,
              "oomphinc/composer-installers-extender": true,
              "drupal/console-extend-plugin": true,
              "zaporylie/composer-drupal-optimizations": true,
              "wikimedia/composer-merge-plugin": true,
              "dealerdirect/phpcodesniffer-composer-installer": true
            }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@patch-files"
        ],
        "patch-files": [
            "php -r \"copy('https://raw.githubusercontent.com/digitalutsc/islandora_lite_installation/main/templates/mods.html.twig', './web/modules/contrib/rest_oai_pmh/templates/mods.html.twig');\""
        ]
    },
    "extra": {
      "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
              "[web-root]/sites/default/settings.php": {
                  "mode": "append",
                  "default": "web/core/assets/scaffold/files/default.settings.php",
                  "append": "assets/patches/default_settings.txt"
              }
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "patches": {
            "mjordan/islandora_workbench_integration": {
              "workbench_integration.patch": "https://raw.githubusercontent.com/digitalutsc/islandora_lite_installation/main/patches/workbench_integration.patch"
            },
            "drupal/views_flipped_table": {
              "accessibility_views_flipped_table_convert_to_layout_table.patch": "https://raw.githubusercontent.com/digitalutsc/islandora_lite_installation/main/patches/accessibility_views_flipped_table_convert_to_layout_table.patch"
            },
            "drupal/ableplayer": {
              "ableplayer_fixentitydisplayissue.patch": "https://raw.githubusercontent.com/digitalutsc/islandora-sandbox/config_sync_context/assets/patches/ableplayer_fixentitydisplayissue.patch"
            },
            "islandora/openseadragon": {
              "openseadragon.authentication.patch": "https://raw.githubusercontent.com/digitalutsc/private_files_adapter/main/scripts/openseadragon.authentication.patch"
            },
            "drupal/file_entity": {
              "override_file_access.patch": "https://raw.githubusercontent.com/digitalutsc/override_permission_file_entity/main/override_file_access.patch"
            },
            "drupal/matomo": {
                "matomo.patch": "https://raw.githubusercontent.com/digitalutsc/dsu_sites_config/main/all/patches/matomo.patch"
            },
            "drupal/better_social_sharing_buttons":{
              "custom_link.patch": "https://www.drupal.org/files/issues/2023-11-09/custom_link_4-0-7-2.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "composer_site.json"
            ]
         }
    }
}
